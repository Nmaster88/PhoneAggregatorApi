services:
  web:
    build:
      args:
        WEB_TAG: v1.0.0
      context: C:\dev\AggregatorAPI
      dockerfile: Dockerfile
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_URLS: https://+:443;http://+:80
    image: testapi
    ports:
    - published: 8080
      target: 80
    - published: 8081
      target: 443
    volumes:
    - C:\Users\nunom\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\nunom\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
version: '3.4'